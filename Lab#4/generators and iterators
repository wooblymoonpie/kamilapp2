   "source": [
    "def squares(a, b):\n",
    "    for i in range(a, b + 1):\n",
    "        yield i ** 2\n",
    "\n",
    "a, b = map(int, input(\"Enter two numbers (a b): \").split())\n",
    "for square in squares(a, b):\n",
    "    print(square)\n"
   ]
  },
