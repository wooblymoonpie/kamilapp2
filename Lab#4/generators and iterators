   "outputs": [],
   "source": [
    "def countdown(n):\n",
    "    for i in range(n, -1, -1):\n",
    "        yield i\n",
    "\n",
    "n = int(input(\"Enter a number: \"))\n",
    "for num in countdown(n):\n",
    "    print(num, end=\" \")\n"
   ]
